<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:controller="com.kanyojozsef96.controller.MainWindowController" styleClass="root" stylesheets="@../css/mainWindow.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox styleClass="container">
         <children>
            <Label styleClass="intro-text" text="Chat App Admin Platform" />
            <HBox styleClass="sub-container">
               <children>
                  <Button onAction="#onListUsersBtnClicked" mnemonicParsing="false" styleClass="btn" text="List Users" />
                  <Button onAction="#onListRoomsBtnClicked" mnemonicParsing="false" styleClass="btn" text="List Rooms" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
